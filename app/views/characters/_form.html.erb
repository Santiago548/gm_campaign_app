<h1>hi</h1>

<%= form_for :character do |f| %>
  NAME: <%= f.text_field :name %><br />
  LEVEL: <%= f.select :level, (1..20) %><br />
  RACE: <%= f.select :race, (character_race) %><br />
  CLASS: <%= f.select :character_class, (character_classes) %><br />
  ARMOR CLASS: <%= f.select :armor_class, (10..22)  %><br />
  SPEED: <%= f.select :speed, ([5,10,15,20,25,30,35,40,45,50]) %> ft<br />
  STRENGTH: +<%= f.select :strength, (1..6) %><br />
  DEXTERITY: +<%= f.select :dexterity, (1..6) %><br />
  CONSTITUTION: +<%= f.select :constitution, (1..6) %><br />
  INTELLIGENCE: +<%= f.select :intelligence, (1..6) %><br />
  WISDOM: +<%= f.select :wisdom, (1..6) %><br />
  CHARISMA: +<%= f.select :charisma, (1..6) %><br />
  LANGUAGES:<br />
    <%= f.fields_for :languages do |lang| %>
      <%= lang.label :language_1 %> 
      <%= lang.select :language_1, (character_language) %><br />
      <%= lang.label :language_2 %> 
      <%= lang.select :language_2, (character_language) %><br />
      <%= lang.label :language_3 %>
      <%= lang.select :language_3, (character_language) %><br />
    <% end %>
  WEAPON PROFICENCY:<br />
    <%= f.fields_for :proficiencies do |weap| %>
      <%= weap.label :weapon_1 %>
      <%= weap.select :weapon_1, (character_weapons) %><br />
      <%= weap.label :weapon_2 %>
      <%= weap.select :weapon_2, (character_weapons) %><br />
    <% end %>
  ARMOR PROFICENCEY:<br />
    <%= f.fields_for :proficiencies do |arm| %>
      <%= arm.label :armor_1 %>
      <%= arm.select :armor_1, (character_armor) %><br />
      <%= arm.label :armor_2 %>
       <%= arm.select :armor_2, (character_armor) %><br />
    <% end %>
  SKILLS:<br />
    <%= f.fields_for :proficiencies do |sk| %>
      <%= sk.label :skill_1 %>
      <%= sk.select :skill_1, (character_skills) %><br />
      <%= sk.label :skill_2 %>
      <%= sk.select :skill_2, (character_skills) %><br />
    <% end %>
    <%= f.collection_select :user_id, User.order(:username),:id,:username, include_blank: true %><br />
    <%= f.collection_select :campaign_id, Campaign.order(:name),:id,:name, include_blank: true %><br />
  <%= f.submit %>
<% end %>