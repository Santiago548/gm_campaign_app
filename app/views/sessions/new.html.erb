<% flash.each do |type, msg| %>
  <div>
    <h2>*<%=type.upcase%>* <%= msg %></h2>
  </div>
<% end %>
LOG-IN BELOW 
<%= form_with(url: '/login') do |f| %>
    <%= f.label :email %>
    <%= f.email_field :email %>
    <%= f.label :password %>
    <%= f.password_field :password %>
    <%= f.submit "Log in" %>
<% end %>
<br>
<%= link_to "Sign up", '/users/new' %>
