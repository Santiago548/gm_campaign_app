<% if @user.game_master == true %>
<h1>Game Master Name: <%= @user.first_name %> <%= @user.last_name %></h1>
<% else %>
   <h1>Player Name: <%= @user.first_name %> <%= @user.last_name %></h1>
<% end %>
<ul>
<li><h3>Username: <%= @user.username %></h3></li>
<li><h3>Email: <%= @user.email %></h3></li>
<li><h3>Phone Number: <%= @user.phone_number %></h3></li>
</ul> 
 <% if current_user && logged_in? %>
    <%= link_to "Edit Your Information", edit_user_path(@user) %>
    <%= button_to("Delete Account", user_path(@user), method: :delete) %>
 <% end %>