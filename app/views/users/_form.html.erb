<fieldset>
<h2> SIGN UP BELOW:</h2>
</fieldset>
<br>
<fieldset>
<%= form_for @user do |f| %>
  Username: <%= f.text_field :username%><br />
  E-mail : <%= f.email_field :email %><br />
  Password: <%= f.password_field :password %><br />
  First-Name: <%= f.text_field :first_name %> 
  Last-Name: <%= f.text_field :last_name %><br />
  Phone Number: <%= f.phone_field :phone_number %><br />
  Game Master: <%= f.check_box :game_master %><br />
  <%= f.submit %>
<% end %>
</fieldset>