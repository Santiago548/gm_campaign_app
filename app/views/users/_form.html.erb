
<%= form_for :user do |f| %>
  Username: <%= f.text_field :username %><br />
  E-mail : <%= f.email_field :email %><br />
  Password: <%= f.password_field :biography %><br />
  name: <%= f.text_field :name %><br />
  Age: <%= f.text_field :age %><br />
  Phone#: <%= f.phone_field :phone_number %><br />
  Game-Master?    : <%= f.check_box :game_masater %><br />
  <%= f.submit %>
<% end %>