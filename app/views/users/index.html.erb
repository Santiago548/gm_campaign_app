<%= user_header %><br>

    <% @users.each do |u| %>
    <hr>
    <fieldset>
        <legend>
        <% if u.game_master == true %>
            <b><%= "GAME MASTER" %></b>
        <% else %>
            <b><%= "PLAYER" %></b>
        <% end %>
            Username: <%=link_to(u.username,  "users/#{u.id}") %><br />
             </legend>
            Name: <%= u.first_name %> <%= u.last_name %><br />
            Email: <%= u.email %><br />
        </fieldset>
    <% end %>
<hr>