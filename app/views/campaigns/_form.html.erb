<%= form_for :campaign do |f| %>
  Campaign Name: <%= f.text_field :name %><br />
  Game Master Name: <%= f.text_field :gm_name %><br />
  Game Master E-mail: <%= f.email_field :gm_email %><br />
  Game Master Phone: <%= f.phone_field :gm_phone_number %><br />
  Campaign Start Date: <%= f.date_field :start_date %><br />
  Campaign End Date: <%= f.date_field :end_date %><br />
  Next Session Date: <%= f.date_field :next_session %><br />
  Quest Log: <%= f.text_area :quest_log %><br />
  Campaign Active: <%= f.check_box :session_active %><br />
  <%= f.submit %>
<% end %>